# HutBite Environment Variables
# Copy this file to .env and update the values as needed

# Backend URL for HubRise orders and deliverability checking
# Default: https://hutbiteintegrations.onrender.com
EXPO_PUBLIC_BACKEND_URL=https://hutbiteintegrations.onrender.com

# Ordering functionality toggle
# Set to 'false' to disable ordering features in production
EXPO_PUBLIC_ORDERING_ENABLED=true
